  <section class="section-form">
  <div id="container-form-content">
    <div class="content-form">
{{#if rus}}      
    <h1>&bull; Расскажите нам о Ваших бизнес-задачах и мы предложим наиболее оптимальное решение &bull;</h1>
{{else}}    
    <h1>&bull; Tell us about Your business tasks and we will offer the most optimal solution &bull;</h1>
{{/if}}    
    <div class="underline">
    </div>
   
    <div class="img_wrapper-form">
      <img src="/img/forma.png">
    </div>
    </div>
{{#if rus}}    
    <form action="#" method="post" id="contact_form">
      <div class="name">
        <label for="name"></label>
        <input type="text" placeholder="Ваше Имя" name="name" id="name_input" required>
      </div>
      <div class="email">
        <label for="email"></label>
        <input type="email" placeholder="Ваш E-mail" name="email" id="email_input" required>
      </div>
      <div class="telephone">
        <label for="name"></label>
        <input type="text" placeholder="Ваш телефон" name="telephone" id="telephone_input" required>
      </div>
      <div class="subject">
        <label for="subject"></label>
        <select placeholder="Тема" name="subject" id="subject_input" required>
          <option disabled hidden selected>Тема</option>
          <option>Web-разработка</option><br>
          <option>Мобильная разработка</option><br>
          <option>Аутсорсинг</option><br>
          <option>Консалтинг</option><br>
          <option>Инфраструктурные проекты</option><br>
          <option>Партнерство</option><br>
          <option>Другое</option><br>
        </select>
      </div>
      <div class="message">
        <label for="message"></label>
        <textarea name="message" placeholder="Вас интересует...." id="message_input" cols="30" rows="5" required></textarea>
      </div>


      <div class="checkbox">
        <div class="checkbox-blok">
        <label class="checkbox-control"></label>
        <input type="checkbox" name="Checkbox" value="yes" class="t-checkbox" data-tilda-req="1" required>
        <div class="checkboz-indicator" style="border-color:#ffffff;"></div>
        <div class="checkbox-labeltext" style="display: inline">Ознакомлен с 
          <a href="politics"> <u>Политикой в области обработки персональных данных</u>
          </a>
          и даю
          <a href="personal"> <u>согласие на их обработку</u>
          </a>
        </div>
        </label>
        </div>
      </div>


      <div class="submit">
        <input type="submit" value="Отправить" id="form_button" />
      </div>
    </form><!-- // End form -->
{{else}}
    <form action="#" method="post" id="contact_form">
      <div class="name">
